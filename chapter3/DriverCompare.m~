t_h = zeros(1, 10);
t_g = zeros(1, 10);
for i=1:10
    m = i*100;
    A = rand(m, m/2);
    b = rand(m, 1);
    h = @() Householder(A, b);
    g = @() Givens(A, b);
    t_h(i) = timeit(h);
    t_g(i) = timeit(g);
end
n = 100:100:1000;
n_cont = linspace(100,1000);
coef_h = polyfit(n, t_h, 3);
coef_g = polyfit(n, t_g, 3);
poly_h = polyval(coef_h, n_cont);
poly_g = polyval(coef_g, n_cont);

plot(n, t_h); hold on;
plot(n, t_g); hold on;
plot(poly_h)